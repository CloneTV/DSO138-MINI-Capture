<Application
    x:Class="DSO138_Capture.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:image="using:Windows.UI.Xaml.Media.Imaging"
    xmlns:local="using:DSO138_Capture">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml"/>
                <ResourceDictionary Source="Resources/Colors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <image:BitmapImage x:Key="BadgeBackgroundActive" UriSource="ms-appx:///Assets/BadgeBackgroundActive.png" />
            <image:BitmapImage x:Key="BadgeBackgroundPassive" UriSource="ms-appx:///Assets/BadgeBackgroundPassive.png" />
        </ResourceDictionary>
    </Application.Resources>

</Application>
